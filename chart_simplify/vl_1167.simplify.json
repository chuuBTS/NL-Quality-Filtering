{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "mark": "circle",
    "encoding": {
        "x": {
            "field": "gdpPerCapitaPPP",
            "type": "quantitative"
        },
        "y": {
            "field": "medianIncome",
            "type": "quantitative"
        },
        "size": {
