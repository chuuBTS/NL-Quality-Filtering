{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "mark": "area",
    "encoding": {
        "x": {
            "timeUnit": "year",
            "field": "Date",
            "type": "quantitative"
        },
        "y": {
            "aggregate": "sum",
            "field": "Value",
            "type": "quantitative"
        },
        "color": {
            "field": "Type",
            "type": "nominal"
        }
    },
    "data": {
        "url": "https://raw.githubusercontent.com/kate-lucas/kate-lucas.github.io/main/finalfishdata.csv"
    },
    "height": 300,
    "width": 300
}