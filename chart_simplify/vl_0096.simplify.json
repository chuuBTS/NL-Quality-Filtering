{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "mark": "rect",
    "encoding": {
        "x": {
            "field": "date",
            "timeUnit": "date",
            "type": "temporal"
        },
        "y": {
            "field": "date",
            "timeUnit": "month",
            "type": "temporal"
        },
        "color": {
            "field": "temp_max",
            "aggregate": "max",
            "type": "quantitative"
        }
    },
    "data": {
        "url": "https://vega.github.io/vega-datasets/data/seattle-weather.csv"
    }
}