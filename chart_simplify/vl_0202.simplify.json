{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "mark": "line",
    "encoding": {
        "y": {
            "field": "Worldwide Gross",
            "type": "quantitative",
            "aggregate": "sum"
        },
        "color": {
            "field": "Genre",
            "type": "nominal"
        },
        "x": {
            "field": "Release Year",
            "type": "quantitative"
        }
    },
    "data": {
        "url": "https://raw.githubusercontent.com/nl4dv/nl4dv/master/examples/assets/data/movies-w-year.csv",
        "format": {
            "type": "csv"
        }
    }
}