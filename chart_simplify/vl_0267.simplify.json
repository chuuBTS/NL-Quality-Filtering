{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "mark": "line",
    "encoding": {
        "x": {
            "timeUnit": "year",
            "field": "date",
            "type": "temporal"
        },
        "y": {
            "aggregate": "mean",
            "field": "price",
            "type": "quantitative"
        },
        "color": {
            "field": "symbol",
            "type": "nominal"
        }
    },
    "data": {
        "url": "https://raw.githubusercontent.com/vega/vega-datasets/master/data/stocks.csv"
    }
}