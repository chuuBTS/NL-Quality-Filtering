{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "mark": "area",
    "encoding": {
        "x": {
            "type": "quantitative",
            "field": "Year"
        },
        "y": {
            "aggregate": "sum",
            "field": "Oil Consumption - TWh (zero filled)",
            "type": "quantitative"
        },
        "color": {
            "field": "Entity",
            "type": "nominal"
        }
    },
    "data": {
        "url": "https://raw.githubusercontent.com/harrywright25/harrywright25.github.io/main/total-share-renewable-w9.csv"
    },
    "height": 250,
    "width": 300
}