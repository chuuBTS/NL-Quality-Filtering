{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "mark": "area",
    "encoding": {
        "x": {
            "timeUnit": "year",
            "field": "Time",
            "type": "quantitative"
        },
        "y": {
            "aggregate": "sum",
            "field": "Value",
            "type": "quantitative"
        },
        "color": {
            "field": "Region",
            "type": "nominal"
        }
    },
    "data": {
        "url": "https://raw.githubusercontent.com/RDeconomist/RDeconomist.github.io/main/charts/environment/greenChart4.csv"
    },
    "height": 300,
    "width": 280
}