{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "mark": "bar",
    "encoding": {
        "y": {
            "field": "Salary",
            "type": "quantitative",
            "aggregate": "mean"
        },
        "x": {
            "field": "Country",
            "type": "nominal",
            "sort": "-y"
        }
    },
    "data": {
        "url": "https://raw.githubusercontent.com/nl4dv/nl4dv/master/examples/assets/data/euro.csv",
        "format": {
            "type": "csv"
        }
    }
}