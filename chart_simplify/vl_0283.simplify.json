{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "mark": "rect",
    "encoding": {
        "x": {
            "bin": true,
            "field": "moon_phase",
            "type": "quantitative"
        },
        "y": {
            "field": "state",
            "type": "nominal"
        },
        "color": {
            "aggregate": "count",
            "type": "quantitative"
        }
    },
    "data": {
        "url": "https://raw.githubusercontent.com/UIUC-iSchool-DataViz/is445_bcubcg_fall2022/main/data/bfro_reports_fall2022.csv"
    },
    "height": 450,
    "width": 400
}