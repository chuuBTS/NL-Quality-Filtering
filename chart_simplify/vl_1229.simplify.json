{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "mark": "area",
    "encoding": {
        "x": {
            "timeUnit": "year",
            "field": "Year",
            "type": "quantitative"
        },
        "y": {
            "aggregate": "sum",
            "field": "Passengers",
            "type": "quantitative"
        },
        "color": {
            "field": "Category",
            "type": "nominal"
        }
    },
    "data": {
        "url": "https://raw.githubusercontent.com/AlKobayashi/AlKobayashi.github.io/main/wk4scrapingairline_IMPROVED%2BCATEGORY.csv",
        "format": {
            "type": "csv"
        }
    },
    "height": 300,
    "width": 250
}