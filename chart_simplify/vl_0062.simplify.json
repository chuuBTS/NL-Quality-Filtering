{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "mark": "line",
    "encoding": {
        "y": {
            "field": "Weight",
            "type": "quantitative",
            "aggregate": "mean"
        },
        "x": {
            "field": "Year",
            "type": "quantitative"
        }
    },
    "data": {
        "url": "https://raw.githubusercontent.com/nl4dv/nl4dv/master/examples/assets/data/cars-w-year.csv",
        "format": {
            "type": "csv"
        }
    }
}