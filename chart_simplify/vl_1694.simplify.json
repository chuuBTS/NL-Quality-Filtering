{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "mark": "bar",
    "encoding": {
        "x": {
            "field": "date",
            "timeUnit": "month",
            "type": "temporal"
        },
        "y": {
            "aggregate": "mean",
            "field": "precipitation",
            "type": "quantitative"
        }
    },
    "data": {
        "url": "https://vega.github.io/vega-lite/data/seattle-weather.csv"
    },
    "height": 300,
    "width": 400
}