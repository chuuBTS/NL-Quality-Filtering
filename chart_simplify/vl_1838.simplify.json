{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "mark": "bar",
    "encoding": {
        "x": {
            "field": "periodo_ingresso",
            "type": "quantitative"
        },
        "y": {
            "field": "contagem",
            "type": "quantitative",
            "aggregate": "sum"
        },
        "color": {
            "field": "sexo",
            "type": "nominal"
        }
    },
    "data": {
        "url": "https://raw.githubusercontent.com/elasComputacao/raio-x-dados/main/data/dados-processados/ingresso.csv"
    }
}