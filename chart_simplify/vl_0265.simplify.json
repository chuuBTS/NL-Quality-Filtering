{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "mark": "line",
    "encoding": {
        "x": {
            "field": "date",
            "type": "temporal"
        },
        "y": {
            "field": "local_price",
            "type": "quantitative"
        },
        "color": {
            "field": "currency_code",
            "type": "nominal"
        }
    },
    "data": {
        "url": "https://cdn.jsdelivr.net/gh/koaning/justcharts/bigmac.csv",
        "format": {
            "type": "csv"
        }
    }
}